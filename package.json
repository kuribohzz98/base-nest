{
  "name": "base-nest-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:prod": "export NODE_OPTIONS=--max_old_space_size=4096 && nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "set NODE_ENV=development& nest start",
    "start:dev": "set NODE_ENV=development& nest start --watch",
    "start:debug": "set NODE_ENV=development& nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --max-warnings=0",
    "lint:prod": "export NODE_OPTIONS=--max_old_space_size=4096 && eslint \"{src,apps,libs}/**/*.ts\" --max-warnings=0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js",
    "migration:create": "typeorm migration:create -n",
    "migration:run": "typeorm migration:run",
    "entity:create": "typeorm entity:create -n"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.4",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "aws-sdk": "^2.1052.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "ejs": "^3.1.6",
    "heic-convert": "^1.2.4",
    "md5": "^2.3.0",
    "mime": "^3.0.0",
    "mysql2": "^2.3.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.29.3",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/md5": "^2.3.1",
    "@types/mime": "^2.0.3",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/sharp": "^0.29.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.2.5",
    "lint-staged": "^12.1.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --max-warnings=0"
    ]
  }
}
